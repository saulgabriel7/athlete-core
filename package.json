{
  "name": "gym-plan-mcp",
  "version": "1.0.0",
  "description": "MCP Server para gerenciamento de treinos de academia, planos alimentares e perfil do usu√°rio",
  "main": "dist/mcp/server.js",
  "type": "module",
  "scripts": {
    "build": "prisma generate && tsc",
    "postinstall": "prisma generate",
    "start": "node dist/api/server.js",
    "dev": "tsx src/mcp/server.ts",
    "api": "tsx src/api/server.ts",
    "api:start": "node dist/api/server.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "setup": "npm run db:generate && npm run db:push && npm run db:seed && npm run build",
    "railway:setup": "prisma db push && npm run db:seed"
  },
  "keywords": [
    "mcp",
    "gym",
    "workout",
    "fitness",
    "meal-plan",
    "cursor"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@prisma/client": "^5.22.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "prisma": "^5.22.0",
    "sharp": "^0.34.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
